.header-wrapper 
  .header-image.kenburns-top
  .header-body-wrapper
    .header-body
      %h1 Dusze Orokinu
      %h3 Polski Sojusz Warframe 2019
      =image_tag(asset_pack_path('media/images/veil-white.png'))
      %br
      =image_tag(asset_pack_path('media/images/warf.png'))


-# Featured
=render 'featured'

-# Carousel
- if @news.any?
  =render 'carousel'

-# Content
.content-wrapper

  .content-onas
    .container
      .about-us-text
        .images
          =image_tag(asset_pack_path('media/images/all-gold.png'))
          =image_tag(asset_pack_path('media/images/jedno-gold.png'))
        - if @abouts.main.any?
          - @abouts.main.each do |about|
            %h1=about.title
            %h2=about.description
            %p=about.content

  - if @abouts.column_one.any?
    - @abouts.column_one.each do |about|
      .content-mozliwosci.parallax
        .future
          .container
            .about-us-text
              %h1=about.title
              %h2=about.description
              %p=about.content

  - if @abouts.main.any?
    .content-dusze
      .parallax1
        .container
          .row.tag
            - @abouts.column_two.each do |about|
              .col-lg-4.tag-col
                .tag-header
                  %h2=about.title
                .tag-text
                  %p=about.content
          .button-about
            %a.btn.btn-do-bordered{:href  => "/alliance/about"} Dowiedz się więcej
      .dolacz
        .container
          .row
            .col-md-8.mx-auto.text-center.d-flex.justify-content-center
              =image_tag(asset_pack_path('media/images/warframe-gold.png'))




  = render "layouts/footbar"




 
